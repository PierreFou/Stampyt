# Uncomment the next line to define a global platform for your project
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'

install! 'cocoapods'

target 'Stampyt' do

  use_frameworks!

  pod 'SwiftLint'
  pod 'Alamofire', '~> 4.7'

  target 'StampytTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'StampytUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
